    <div class="form-group">
        <label for="username">
            Name
        </label>
        <br>
        <input class="form-control" type="text" name="name" placeholder="Name"> </div>
    <div class="form-group">
        <label for="password">
            Password
        </label>
        <br>
        <input class="form-control" type="password" name="password" placeholder="password"> </div>
    <div class="form-group">
        <label for="email">
            Role
